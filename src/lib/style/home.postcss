.work-web-site {
    & .tabs-page, & .tabs-platform {
        margin-bottom: 16px;
        display: flex;
        gap: 8px;
        justify-content: center;
    }
}

.projects {
    padding-top: 80px;
    & .project-image {
        & img {
            display: block;
            max-width: 100%;
            margin: auto;
            border-radius: 4px;
        }
    }
}

.sketch-plugin-stats {
    display: flex;
    flex-wrap: wrap;
    gap: 16px;
    max-width: 800px;
    margin: 0 auto 48px;
    & li {
        flex: 1;
        text-align: center;
        font-size: 12px;
        color: var(--secondary-text-color);
    }
    & strong {
        display: block;
        font-size: 40px;
        color: transparent;
        background-image: linear-gradient(0deg, #FFDF5B 0%, #FF5900 100%);
        background-clip: text;
    }
}

.sequence-player {
    position: relative;
    max-width: 600px;
    aspect-ratio: 10 / 9;
    margin: auto;
    border-radius: 4px;
    overflow: hidden;
    & img {
        position: absolute;
        display: block;
        width: 100%;
        opacity: 0;
    }
}

.npm-download-chart {
    display: flex;
    max-width: 600px;
    margin: 0 auto 24px;
    border-bottom: 3px solid rgba(137, 86, 255, .2);
    & .chart-text {
        flex: 1;
    }
    & #npm-chart-day {
        color: #999;
        font-size: 12px;
    }
    & #npm-chart-downloads {
        font-size: 32px;
        font-family: number, sans-serif;
    }
    @media (--mobile) {
        flex-direction: column;
    }
}

.code-window {
    max-width: 600px;
    height: 160px;
    margin: auto;
    padding: 16px;
    background: #151718;
    box-shadow: 0 0 2px rgba(0, 0, 0, .2), 0 4px 8px rgba(0, 0, 0, .2), 0 10px 20px rgba(0, 0, 0, .2), 0 20px 40px rgba(0, 0, 0, .2);
    border-radius: 10px;
    text-align: left;
    color: #00CC66;
    font-size: 14px;
    user-select: none;
    & pre {
        padding: 8px;
    }
    & code::after {
        content: "";
        display: inline-block;
        width: 0.5em;
        height: 1em;
        margin: 0 .2em;
        background: #00CC66;
        vertical-align: middle;
        transition-property: none;
        animation: cursor 1s step-end infinite;
    }
}

.plugin-features {
    display: flex;
    flex-wrap: wrap;
    gap: 16px;
    max-width: 800px;
    margin: 0 auto 48px;
    & li {
        flex: 1;
        font-size: 12px;
        text-align: center;
        color: var(--secondary-text-color);
    }
    & img {
        display: block;
        margin: auto;
    }
    @media (--mobile) {
        & li {
            flex-basis: calc((100% - 32px) / 3);
        }
    }
}

@keyframes shake {
    0% { margin-top: 0; }
    50% { margin-top: 8px; }
}
@keyframes weather-cloud {
    0% { x: 64; }
    50% { x: 68; }
}
@keyframes widget-cloud {
    0% { margin-left: 0px; }
    50% { margin-left: 8px; }
}
@keyframes meteor {
    15% { opacity: 0.8; }
    30%, 100% { background-size: 10% 100%; background-position: 0; opacity: 0; }
}
@keyframes star {
    10% { opacity: 0; }
    40% { opacity: 0.8; }
}
@keyframes rotate {
    from { transform: rotate(0deg); }
    to { transform: rotate(360deg); }
}
@keyframes rotate-safe {
    25% { transform: rotate(36deg); }
    50% { transform: rotate(0deg); }
    75% { transform: rotate(-36deg); }
}
@keyframes rotate-compass {
    25% { transform: rotate(10deg); }
    50% { transform: rotate(0deg); }
    75% { transform: rotate(-10deg); }
}
@keyframes rotate-blur {
    0% { transform: rotate(0deg); }
    10% { opacity: 0; filter: blur(2px); }
    100% { transform: rotate(3600deg); }
}
@keyframes matrix {
    0% { background-position-y: 0; }
    50% { background-position-y: 65px; }
    100% { background-position-y: 130px; }
}
@keyframes ripple {
    from { box-shadow: 0 0 0 0 rgba(0, 102, 255, .6); }
    to { box-shadow: 0 0 0 10px rgba(0, 102, 255, 0); }
}
@keyframes mousewheel {
    from { top: 9px; }
    to { top: 4px; opacity: 0.4; }
}
@keyframes cursor {
    0% { display: 1; }
    50% { opacity: 0; }
}