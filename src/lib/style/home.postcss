.about {
    display: flex;
    gap: 48px;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    min-height: 100vh;
    background: #f5f5f5;
    padding: 72px env(safe-area-inset-right) 48px env(safe-area-inset-left);
    & .about-text {
        color: #666;
        max-width: 800px;
        padding: 0 16px 0;
        tex-align: justify;
        & p {
            margin-bottom: 1em;
        }
        & .contact {
            margin-left: 1em;
            list-style-type: disc;
            line-height: 2em;
            & a:link, & a:visited {
                text-decoration: none;
                margin-left: 8px;
            }
        }
        & .wechat-qrcode {
            display: none;
            position: absolute;
            width: 188px;
            height: 262px;
            border-radius: 8px;
            box-shadow: 0 1px 2px 0 rgba(0,0,0,0.04), 0 2px 4px 0 rgba(0,0,0,0.04), 0 4px 8px 0 rgba(0,0,0,0.04), 0 8px 16px 0 rgba(0,0,0,0.04), 0 16px 32px 0 rgba(0,0,0,0.04), 0 32px 64px 0 rgba(0,0,0,0.04);
        }
        & .wechat {
            &::after {
                content: "";
                display: inline-block;
                width: 16px;
                height: 16px;
                background-image: url("data:image/svg+xml,%3Csvg width='24px' height='24px' viewBox='0 0 24 24' version='1.1' xmlns='http://www.w3.org/2000/svg' xmlns:xlink='http://www.w3.org/1999/xlink'%3E%3Cg stroke='none' stroke-width='1' fill='none' fill-rule='evenodd'%3E%3Cg fill='%23000000' fill-rule='nonzero'%3E%3Cpath d='M11,13 L11,21 L3,21 L3,13 L11,13 Z M15,19 L15,21 L13,21 L13,19 L15,19 Z M19,19 L19,21 L17,21 L17,19 L19,19 Z M9.5,14.5 L4.5,14.5 L4.5,19.5 L9.5,19.5 L9.5,14.5 Z M17,17 L17,19 L15,19 L15,17 L17,17 Z M21,17 L21,19 L19,19 L19,17 L21,17 Z M8,16 L8,18 L6,18 L6,16 L8,16 Z M19,15 L19,17 L17,17 L17,15 L19,15 Z M15,15 L15,17 L13,17 L13,15 L15,15 Z M21,13 L21,15 L19,15 L19,13 L21,13 Z M17,13 L17,15 L15,15 L15,13 L17,13 Z M11,3 L11,11 L3,11 L3,3 L11,3 Z M21,3 L21,11 L13,11 L13,3 L21,3 Z M9.5,4.5 L4.5,4.5 L4.5,9.5 L9.5,9.5 L9.5,4.5 Z M19.5,4.5 L14.5,4.5 L14.5,9.5 L19.5,9.5 L19.5,4.5 Z M8,6 L8,8 L6,8 L6,6 L8,6 Z M18,6 L18,8 L16,8 L16,6 L18,6 Z'%3E%3C/path%3E%3C/g%3E%3C/g%3E%3C/svg%3E");
                background-repeat: no-repeat;
                background-size: 16px;
                overflow: hidden;
                margin-left: 8px;
                vertical-align: middle;
            }
            &:hover + .wechat-qrcode {
                display: block;
            }
        }
        & a:link, & a:visited {
            color: #000;
        }
    }
    & .mouse {
        --fill: #999;
        flex-shrink: 0;
        position: relative;
        width: 24px;
        height: 36px;
        border: 2px solid var(--fill);
        border-radius: 12px;
        & p {
            position: absolute;
            top: 9px;
            left: 9px;
            width: 2px;
            height: 0;
            padding-top: 6px;
            overflow: hidden;
            border-radius: 1px;
            background: var(--fill);
            animation: mousewheel 1s linear infinite;
        }
    }
    
}

.works {
    padding: 80px 16px;
    padding-left: calc(env(safe-area-inset-left) + 16px);
    padding-right: calc(env(safe-area-inset-right) + 16px);
    overflow: hidden;
    & .works-logo {
        max-width: 800px;
        margin: 0 auto 48px;
        & img {
            max-width: 100%;
            display: block;
            margin: auto;
        }
    }
    & .works-text {
        max-width: 800px;
        text-align: center;
        margin: 0 auto 48px;
    }
    & .works-title {
        font-size: 42px;
        font-weight: 500;
        line-height: 1.2;
        margin-bottom: 8px;
    }
    & .works-title-small {
        font-size: 24px;
    }
