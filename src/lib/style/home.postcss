        & a:link, & a:visited {
            text-decoration: none;
        }
    }
    & .works-description {
        font-size: 14px;
        & p:not(:last-child) {
            margin-bottom: 1em;
        }
    }
    &.dark .works-info li {
        background-position: 0 -12px;
    }
    & .works-content img {
        display: block;
    }
}

.slide {
    & .slide-page {
        width: 100%;
        user-select: none;
        & .slide-image {
            display: block;
            margin-bottom: 16px;
            width: 100%;
            max-height: 500px;
            aspect-ratio: 2 / 1;
            object-fit: contain;
        }
        & .slide-text {
            text-align: center;
            font-size: 12px;
            color: var(--secondary-text-color);
        }
    }
    & .swiper-pagination-fraction {
        top: 16px;
        bottom: auto;
        left: auto;
        right: 16px;
        width: auto;
        height: 24px;
        font-size: 12px;
        line-height: 24px;
        padding: 0 8px;
        background: rgba(0, 0, 0, .2);
        border-radius: 12px;
        color: #FFF;
        backdrop-filter: blur(5px);
        box-shadow: 0 0 1px 1px rgba(255, 255, 255, .25);
    }
    & .swiper-button-next, & .swiper-button-prev {
        background-color: var(--color-accent);
        backdrop-filter: blur(5px);
        width: 32px;
        height: 32px;
        border-radius: 16px;
        transition: .4s background-color;
        &.swiper-button-disabled {
            opacity: 0.1;
        }
        &:hover {
            animation: ripple 1s ease infinite;
        }
    }
    & .swiper-button-next {
        right: 8px;
        background-image: url("data:image/svg+xml,%3Csvg width='32' height='32' xmlns='http://www.w3.org/2000/svg'%3E%3Cpath d='m15 11 l5 5 -5 5' stroke='%23FFF' stroke-width='2' fill='none' stroke-linecap='round' stroke-linejoin='round'/%3E%3C/svg%3E");
    }
    & .swiper-button-prev {
        left: 8px;
        background-image: url("data:image/svg+xml,%3Csvg width='32' height='32' xmlns='http://www.w3.org/2000/svg'%3E%3Cpath d='m17 21-5-5 5-5' stroke='%23FFF' stroke-width='2' fill='none' stroke-linecap='round' stroke-linejoin='round'/%3E%3C/svg%3E");
    }
    & .swiper-button-next:after, & .swiper-button-prev:after {
        display: none;
    }
}

.slide-multiple {
    & .swiper-slide {
        width: auto;
        border: 1px solid transparent;
    }
    & .slide-page {
        padding-bottom: 16px;
        & .slide-image {
            display: block;
            max-height: auto;
            aspect-ratio: auto;
            margin-bottom: 8px;
            box-shadow: 0 1px 2px 1px rgba(0, 0, 0, .1);
            object-fit: cover;
        }
        & .slide-text {
            text-align: left;
        }
    }
    & .swiper-pagination-progressbar.swiper-pagination-horizontal {
        bottom: 0px;
        top: auto;
        left: 50%;
        width: 30vw;
        transform: translateX(-50%);
        height: 2px;
        background: rgba(0, 0, 0, .1);
        user-select: none;
    }
    & .swiper-pagination-progressbar {
        & .swiper-pagination-progressbar-fill {
            --swiper-pagination-color: var(--color-accent);
            --swiper-theme-color: var(--color-accent);
        }
    }
}

.slide-auto {
    max-width: 600px;
    & .slide-page {
        & .slide-image {
            max-height: 400px;
            aspect-ratio: auto;
            margin: 0;
            border-radius: 4px;
        }
    }
}

.vertical-scroller {
    height: 500px;
    & .swiper-slide {
        height: auto;
    }
    & .slide-image {
        display: block;
        max-width: 100%;
        margin: auto;
        object-fit: contain;
    }
}

.masonry {
    height: 500px;
    & .swiper-slide {
        height: auto;
        column-count: 3;
        column-gap: 8px;
    }
    & .slide-image {
        width: 100%;
        display: block;
        margin-bottom: 8px;
        break-inside: avoid;
    }
    @media (--tablet) {
        & .swiper-slide {
            column-count: 2;
        }
    }
}

.popup-tip {
    position: absolute;
    z-index: 10;
    color: var(--text-color-primary);
    padding: 16px;
    border-radius: 8px;
    animation: shake 1.2s ease-in-out 10;
    &::after {
        content: "";
        position: absolute;
        width: 0;
        height: 0;
        left: calc(50% - 8px);
        bottom: -16px;
        border: 8px solid transparent;
        border-top: 8px solid var(--background-color);
    }
}

.work-theme-1 {
    --background-color: #322C6E;
    & .theme-preview {
        display: flex;
        flex-wrap: wrap;
        justify-content: center;
        gap: 16px;
        margin-bottom: 80px;
    }
    & .theme-icons {
        display: block;
        margin: 0 auto 80px;
        max-width: 100%;
        height: auto;
    }
    & .icon-shape-controller {
        display: flex;
        align-items: center;
        justify-content: center;
        position: relative;
        margin: 32px auto;
        color: #FFFFFF;
        font-size: 12px;
        & svg {
            display: block;
            margin: 0 8px;
            flex-shrink: 0;
        }
        & .icon-shape-smooth {
            position: relative;
            height: 16px;
            margin: 0 8px;
            & input {
                position: absolute;
                top: 0;
                left: 0;
                display: block;
                width: 16px;
                height: 16px;
                opacity: 0;
            }
            & svg {
                margin: 0;
            }
            & input:checked + svg path{
                stroke-opacity: 1;
            }
        }
        & input[type=range] {
            --range-accent: #3F51B5;
            --range-background: #FFF;
            --range-thumb: #FFF;
        }
        & .popup-tip {
            top: -40px;
            left: calc(50% - 144px);
        }
    }
    & .weather-icon {
        display: block;
        margin: 24px auto;
    }
    & .weather-cloud {
        animation: weather-cloud 2s ease infinite;
    }
    & .theme-widget {
        position: relative;
        padding-bottom: 200px;
    }
    & #widget {
        position: relative;
        z-index: 20;
        width: 311px;
        height: 311px;
        margin: 0 auto;
        overflow: hidden;
        font-family: Roboto, sans-serif;
        user-select: none;
        text-align: center;
    }
    & #widget-bg {
        position: absolute;
        top: 0;
        left: 0;
        width: 311px;
        height: 311px;
        background: url("/360os_theme_1/widget_bg.png") no-repeat 0 0;
        background-size: 311px 622px;
        transition: background-color 0.5s ease;
    }
    & #widget-icon {
        position: absolute;
        top: 29px;
        left: 176px;
        width: 96px;
        height: 80px;
        overflow: hidden;
    }
    & #widget-icon-sun, & #widget-icon-moon {
        position: absolute;
        top: 12px;
        left: 20px;
        width: 56px;
        height: 56px;
        border-radius: 28px;
        background: #FFCC33;
        transition: top 1s ease;
    }
    & #widget-icon-moon {
        top: 92px;
        background: #C6D2EC;
    }
    & #widget-icon-cloud {
        position: absolute;
        top: 42px;
        left: 40px;
        width: 42px;
        height: 26px;
        border-radius: 13px;
        background: #FFFFFF;
        animation: widget-cloud 2s ease-in-out infinite;
    }
    & #widget-date {
        position: relative;
        font-size: 14px;
        margin-top: 80px;
        line-height: 20px;
    }
    & #widget-time {
        position: relative;
        font-family: Number, sans-serif;
        font-size: 72px;
        margin-top: 8px;
        line-height: 1;
    }
    & #widget-weather {
        position: relative;
        font-size: 14px;
        margin-top: 8px;
        line-height: 20px;
    }
    & #widget-sky {
        opacity: 0;
        transition: opacity 1s ease;
    }
    & .meteor {
        position: absolute;
        z-index: 10;
        bottom: 0;
        left: 0;
        width: 200px;
        height: 2px;
        overflow: hidden;
        border-radius: 1px;
        transform: rotate(-45deg);
        background-image: linear-gradient(-45deg, rgba(255,255,255,0), rgba(255,255,255));
        background-repeat: no-repeat;
        background-size: 50% 100%;
        background-position: 100% 0;
        opacity: 0;
    }
    & .star {
        position: absolute;
        z-index: 10;
        bottom: 0;
        left: 0;
        width: 2px;
        height: 2px;
        overflow: hidden;
        border-radius: 1px;
        background: #fff;
        opacity: 0.8;
    }
    @media (--mobile) {
        & .theme-preview {
            flex-direction: column;
            align-items: center;
        }
        & .icon-shape-controller {
            & input[type=range] {
                width: 120px;
            }
        }
    }
}

.work-theme-2 {
    --background-color: #313163;
    & .theme-wrap {
        position: relative;
        max-width: 1160px;
        height: auto;
        margin: auto;
    }
    & img {
        width: 100%;
        height: auto;
        margin: auto;
    }
    & .theme-icon, & .theme-icon-layer {
        position: absolute;
        top: 0;
        left: 0;
        width: 10.34482759%;
        aspect-ratio: 1;
        background-size: 100%;
        background-repeat: no-repeat;
        transition-property: all;
    }
    & .theme-icon-layer {
        width: 100%;
    }
    & .theme-icon-clean {
        top: 36.363636363636365%; 
        left: 51.03448275862069%;
